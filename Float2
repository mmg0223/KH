package type;

public class FloatTest02 {
	
	public static void main(String[] args) {
		//실수형은 기본형이 double일까
		float result = 0;//0.0f
		//메모리공간 = 변수값
		/*
		result = result + 0.1f;
		result = result + 0.1f;
		result = result + 0.1f;
		
		System.out.println(result);
		*/
		
		
		//반복문
		for(int i = 0; i<100; i++) {
			result = result + 0.1f;
		}
		
		System.out.printf("%.6f",result);
		
	}

}
