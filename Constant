package integer;

public class ConstantTest {
	//상수 : 변수의 이름은 대문자로 표기하는게 일반적
	//final 에서만 상수가 사용되는 것이 아닌 공유의 목적으로 사용
	static final double PI = 3.141592; //필드
	//final double AA;
	public static void main(String[] args) {
		//수정불가의 의미를 지니고 있음
		
		//ConstantTest.PI = 10;//final field는 수정불가
		
		System.out.println(Math.PI);
	}

}
