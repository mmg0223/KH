package integer;

public class LiteralTest {

	public static void main(String[] args) {
		
		long num1 = 2200000000L;
		float num2 = 3.14F;
		
		int num3 = 017;//8진수는 숫자 앞에 0을 붙임
		System.out.println(num3);
		
		int num4 = 0x10;//16진수는 숫자 앞에 0x를 붙임
		System.out.println(num4);
		num4 = 0xF;
		System.out.println(num4);

	}

}
